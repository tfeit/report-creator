    "echarts": "^6.0.0",
    "next": "^16.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.3",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3"
